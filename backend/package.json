{
  "name": "hardhat-project",
  "scripts": {
    "chain": "npx hardhat node",
    "compile": "npx hardhat compile",
    "clean": "npx hardhat clean",
    "console:local": "npx hardhat console --network localhost",
    "console:rinkeby": "npx hardhat console --network rinkeby",
    "deploy:localhost": "npx hardhat run --network localhost scripts/deploy.js",
    "deploy:rinkeby": "npx hardhat run --network rinkeby scripts/deploy.js",
    "deploy": "npx hardhat run --network mainnet scripts/deploy.js"
  },
  "devDependencies": {
    "@nomiclabs/hardhat-ethers": "^2.0.4",
    "@nomiclabs/hardhat-waffle": "^2.0.1",
    "chai": "^4.3.4",
    "ethereum-waffle": "^3.4.0",
    "ethers": "^5.5.3",
    "hardhat": "^2.8.2"
  },
  "dependencies": {
    "@openzeppelin/contracts": "^4.4.2",
    "@supercharge/filesystem": "^2.3.0",
    "dotenv": "^14.2.0",
    "fs-extra": "^10.0.0"
  }
}
